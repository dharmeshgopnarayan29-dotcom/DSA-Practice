     "output_type": "stream",
     "text": [
      "36\n",
      "12\n"
     ]
    }
   ],
   "source": [
    "# LCF (Least Common factor)\n",
    "\n",
    "def find_lcm(a, b):\n",
    "    return (a * b) // find_hcf(a, b)\n",
    "\n",
    "print(find_lcm(12, 36))\n",
    "print(find_hcf(12, 36))"
   ]
  }
