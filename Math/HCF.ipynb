   "source": [
    "# HCF (highest common factor)\n",
    "\n",
    "def find_hcf(a, b):\n",
    "    while b!=0:\n",
    "        a,b = b,a%b\n",
    "    return a\n",
    "print(find_hcf(24,36))"
   ]
  },
  {
   "cell_type": "code",
   "execution_count": 5,
   "id": "3209bae1",
   "metadata": {},
   "outputs": [
    {
